<Project xmlns="http://schemas.microsoft.com/developer/msbuild/2003">

  <PropertyGroup>
    <MSBuildAllProjects>$(MSBuildAllProjects);$(MSBuildThisFileFullPath)</MSBuildAllProjects>
  </PropertyGroup>

  <!--
    Package Version property for Implicit Packages included in the props file
  -->
  <PropertyGroup Condition="'$(TargetFrameworkIdentifier)' == '.NETCore' AND '$(_SdkTargetFrameworkVersionWithoutV)' >= '5.0' AND '$(ExtrasImplicitPlatformPackageVersion)' == ''">
    <ExtrasImplicitPlatformPackageVersion>6.1.5</ExtrasImplicitPlatformPackageVersion>
  </PropertyGroup>

  <PropertyGroup Condition="'$(TargetFrameworkIdentifier)' == 'Tizen' AND '$(ExtrasImplicitPlatformPackageVersion)' == ''">
    <ExtrasImplicitPlatformPackageVersion Condition="'$(_SdkTargetFrameworkVersionWithoutV)' == '3.0'">3.0.0</ExtrasImplicitPlatformPackageVersion>
    <ExtrasImplicitPlatformPackageVersion Condition="'$(_SdkTargetFrameworkVersionWithoutV)' == '4.0'">4.0.0</ExtrasImplicitPlatformPackageVersion>
  </PropertyGroup>

</Project>