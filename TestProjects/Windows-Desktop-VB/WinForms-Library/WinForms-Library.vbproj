<Project Sdk="MSBuild.Sdk.Extras">

  <PropertyGroup>
    <TargetFramework>net45</TargetFramework>
    <ExtrasEnableWinFormsProjectSetup>true</ExtrasEnableWinFormsProjectSetup>
    <ExtrasEnableUserControlDesigner>true</ExtrasEnableUserControlDesigner>
    <MyType>Windows</MyType>
    <DefineConstants>_MYFORMS=True</DefineConstants>
  </PropertyGroup>

  <PropertyGroup>
    <AssemblyName>WinFormsClassLib</AssemblyName>
    <AssemblyTitle>Windows Forms Library</AssemblyTitle>
  </PropertyGroup>

  <ItemGroup>
    <ProjectReference Include="..\Framework-Library\Framework-Library.vbproj" />
  </ItemGroup>

</Project>
